<%= render 'primary_navigation' %>
<%= render 'consumer_navigation' %>

<section id="maincontainer">

	<%= render 'page_head', :page_title => "Purchase Parameters", :backPath => consumer_purchase_path(@consumer, @purchase) %>
	
	<table>
		<tbody>
			<% @params.each do |key, value| %>
			  <% next if ['controller', 'action', 'utf8','authenticity_token', 'commit'].include? key %>
			  <tr>
			    <td><%= key %></td>
			    <td><%= value %></td>
			  </tr>
			<% end %>
		</tbody>
	</table>
	
	<br />

</section>

<%= render 'notifications' %>