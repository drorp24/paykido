$('#salutation').html('<%= @salutation %>');
$('#name').html('<%= @name %>');
<% if @pic %>
	$('#pic').html('<%= image_tag @pic %>');
<% else %>
	$('#pic').empty();
<% end %>
$('#ajax_message #first_line').html("<%= @first_line %>");
$('#ajax_message #second_line').html("<%= @second_line %>");
$('#login_button').unbind('click');
$('#login_button').hide();
$('#registration').attr('style', 'display:block');
