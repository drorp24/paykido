<% unless params[:_pjax] %>
	<%= render 'primary_navigation'  %>
	<%= render 'setting_navigation' %>

<section id="maincontainer">
	<div id="main" class="container_12">
<% end %>

<%= render :partial => 'page_head', :locals => {:title => 'Registration Details', :backPath => payer_registrations_path(@payer)} %>
<table>
	<tbody>
		<% @registration.attributes.each do |name, value| %>
		  <tr>
		    <td>Card Number</td>
		    <td><%= @registration.cardNumber %></td>
		  </tr>
		  <% next if name == 'id' or name == 'payer_id' or name == 'cardNumber' or !value %>
		  <tr>
		    <td><%= name %></td>
		    <td><%= value %></td>
		  </tr>
		<% end %>
	</tbody>
</table>

<br />
<% unless params[:_pjax] %>
  </div>
</section>
<% end %>
