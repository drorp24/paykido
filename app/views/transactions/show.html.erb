<% unless params[:_pjax] %>
	<%= render 'primary_navigation'  %>
	<%= render 'consumer_navigation' %>

<section id="maincontainer">
	<div id="main" class="container_12">
<% end %>

<%= render :partial => 'page_head', :locals => {:title => 'Transaction Details'} %>
<table>
	<tbody>
		  <tr>
		    <td>Type</td>
		    <td><%= @transaction.trx_type %></td>
		  </tr>
		<% @transaction.attributes.each do |name, value| %>
		  <% next if name == 'id' or name == 'payer_id' or name == 'trx_type' or !value %>
		  <tr>
		    <td><%= name %></td>
		    <td><%= value %></td>
		  </tr>
		<% end %>
	</tbody>
</table>

<br />
<% unless params[:_pjax] %>
  </div>
</section>
<% end %>
