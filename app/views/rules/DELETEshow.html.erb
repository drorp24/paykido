<%= render 'primary_navigation'  %>
<%= render 'consumer_navigation' %>

<section id="maincontainer">

	<%= render :partial => 'page_head', :locals => {:title => 'Rule Details', :backPath => :back} %>
	
	<table>
		<tbody>
			<% @rule.attributes.each do |name, value| %>
			  <% next if name == 'id' or name == 'payer_id' or !value %>
			  <tr>
			    <td><%= name %></td>
			    <td><%= value %></td>
			  </tr>
			<% end %>
		</tbody>
	</table>
	

</section>

<%= render 'notifications' %>